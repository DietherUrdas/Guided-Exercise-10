<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <Button
        android:id="@+id/btnOpenURLGE10"
        android:layout_width="50dp"
        android:layout_height="48dp"
        app:icon="?android:attr/actionModeWebSearchDrawable"
        app:iconGravity="textTop"
        app:iconSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/actvURLGE10"
        app:layout_constraintTop_toTopOf="parent" />


    <AutoCompleteTextView
        android:id="@+id/actvURLGE10"
        android:layout_width="359dp"
        android:layout_height="52dp"
        android:hint="Search or Enter Website Name"
        app:dropDownBackgroundTint="#8000BCD4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="410dp"
        android:layout_height="666dp"
        app:layout_constraintTop_toBottomOf="@+id/actvURLGE10"
        tools:layout_editor_absoluteX="2dp" />

    <Button
        android:id="@+id/back"
        android:layout_width="77dp"
        android:layout_height="45dp"
        app:icon="?attr/homeAsUpIndicator"
        app:iconSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.331"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.916" />

    <Button
        android:id="@+id/home"
        android:layout_width="78dp"
        android:layout_height="45dp"
        app:icon="?android:attr/listChoiceIndicatorSingle"
        app:iconSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.916" />

</androidx.constraintlayout.widget.ConstraintLayout>
